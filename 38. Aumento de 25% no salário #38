Var
   S,A,NS: Real


Inicio
      Escreva("Digite o salário do funcionário: ")
      Leia(S)
      Escreval("Aplicando um aumento de 25% ao salário de",S,"R$")
      A<- (S/100)*25
      NS <- (S+A)
      Escreva("O novo salário é",NS,"R$")











