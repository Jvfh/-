Var
    VH,QH,SI: Real


Inicio
      Escreva("Digite o valor da hora de trabalho: R$")
      Leia(VH)
      Escreva("Quantidade de horas trabalhadas no mês: ")
      Leia(QH)
      SI <- (VH*QH)+(VH*QH)*0.1)
      Escreva("O salário final com um acrecimo de 10% é:",SI,"R$")


Fimalgoritmo
