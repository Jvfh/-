Var
    A,R,V: real


Inicio
      Escreva("Digite a altura do cilindro: ")
      Leia(A)
      Escreva("Digite o raio do cilindro: ")
      Leia(R)
      V <- (A*3.14)*(R^2)
      Escreva("O volume do cilindro é:",V)
      








