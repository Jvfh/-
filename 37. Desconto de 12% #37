// Data atual  : 04/03/2020
Var
   V,D,VF: Real


Inicio
      Escreva("Digite o valor a ser aplicado o desconto de 12%: ")
      Leia(V)
      D <- (V/100)*12
      Escreval("O valor do desconto é de:",D,"R$")
      VF <- (V-D)
      Escreva("Logo você tera que pagar cerca de:",VF,"R$")









Fimalgoritmo
