Var
   TC,TF,C: Real


Inicio
      Escreva("Diga a temperatura em Fahrenheit: ")
      Leia(TF)
      C <- (5.0 * (TF - 32.0)/9.0)
      Escreva("A temperatura",TF," em Fahrenheit Convertida para Celsius é ",C:4:1)
      



Fimalgoritmo
