Var
   AB,H,A: Real
   

Inicio
      Escreva("Digite a área da base do quadrado: ")
      Leia(AB)
      Escreva("Digite a altura do quadrado: ")
      Leia(H)
      A <- (AB*H)
      Escreva("A área do quadrado é ",A)


      




