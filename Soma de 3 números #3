// Data atual  : 03/03/2020
Var
   N1,N2,N3,S: Inteiro


Inicio
      Escreva("Digite o primeiro número: ")
      Leia(N1)
      Escreva("Digite o segundo número: ")
      Leia(N2)
      Escreva("Digite o terceiro número: ")
      Leia(N3)
      S <- (N1+N2+N3)
      Escreva("A soma dos números é",S)


Fimalgoritmo
