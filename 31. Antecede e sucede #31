Var
   N,X,Y: inteiro
   

Inicio
      Escreva("Digite um número: ")
      Leia(N)
      X <- (N-1)
      Y <- (N+1)
      Escreva("O número ",X," antecede o número ",N," assim como ",Y," o sucede  ")

      




