Var
   A,B,H: real
   

Inicio
      Escreva("Valor do cateto A: ")
      Leia(A)
      Escreva("Valor do cateto B: ")
      Leia(B)
      H <- RaizQ((A^2)+(B^2))
      Escreva("O Hipotenusa desses catetos é: ",H:4:1)

      
      



      


Fimalgoritmo
